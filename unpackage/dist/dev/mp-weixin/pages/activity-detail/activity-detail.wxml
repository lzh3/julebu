<view class="{{['container data-v-0e0acaa6','activity-detail',activityStatus]}}"><block wx:if="{{isLogin}}"><view class="activity-wrap container data-v-0e0acaa6"><not-login vue-id="9961e9c0-1" pageName="活动" class="data-v-0e0acaa6" bind:__l="__l"></not-login></view></block><block wx:else><view class="data-v-0e0acaa6"><image class="pic data-v-0e0acaa6" src="{{item.image}}"></image><view class="content data-v-0e0acaa6"><view class="item data-v-0e0acaa6">活动主题：<text class="theme data-v-0e0acaa6">{{item.title}}</text><text class="status data-v-0e0acaa6">{{''+(item.status==='in_start'?'进行中':item.status==='end'?'已结束':'未开始')+''}}</text></view><view class="item data-v-0e0acaa6">活动时间：<text class="{{['date data-v-0e0acaa6',(end)?'end':'']}}">{{''+item.start_time+"-"+item.end_time+''}}</text></view><view class="item data-v-0e0acaa6">活动形式：<text class="{{['type data-v-0e0acaa6',(end)?'end':'']}}">{{item.type}}</text></view><view class="item data-v-0e0acaa6">活动介绍：<view class="desc data-v-0e0acaa6">{{item.description}}</view></view><block wx:if="{{joined}}"><button class="{{['btn data-v-0e0acaa6',(item.status!=='in_start')?'over':'']}}" type="primary" disabled="{{item.status!=='in_start'}}" data-event-opts="{{[['tap',[['joinActivity',['$event']]]]]}}" bindtap="__e">我要参与</button></block></view><kefu vue-id="9961e9c0-2" class="data-v-0e0acaa6" bind:__l="__l"></kefu></view></block><modal vue-id="9961e9c0-3" status="{{modalStatus}}" title="参与成功" desc data-ref="modal" class="data-v-0e0acaa6 vue-ref" bind:__l="__l"></modal></view>